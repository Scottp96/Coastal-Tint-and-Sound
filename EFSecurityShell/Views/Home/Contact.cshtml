@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title</h2>
<h3>Location</h3>

<div id="googleMap" style="width:100%;height:400px;"></div>

<script>
    function myMap() {
        var mapProp = {
            center: new google.maps.LatLng(38.769044, -75.217057),
            zoom: 9,
        };
        var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);
        var marker = new google.maps.Marker({
            position: new google.maps.LatLng(38.769044, -75.217057),
            map: map,
            title: 'Coastal Dealership!'
        });
    }
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBOUUzioaaSwp6yDX3swrUiwpGPED7Wf4M&callback=myMap"></script>

